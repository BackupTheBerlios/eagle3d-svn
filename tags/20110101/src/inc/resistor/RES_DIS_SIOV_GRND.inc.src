        union{
            superellipsoid {<1.0, 0.6> scale <b/2,b/2,s/2> }
            intersection{
                superellipsoid {<1.0, 0.6> scale <b/2,b/2,s/2+0.01> }
                union{
                    text {ttf global_fontfile_arial value 1, 0 scale 1 rotate<0,0,0> translate<-2,+1,-s/2>}
