                    scale<b/9,b/9,1>
                    
                }            
                pigment{White} 
            }
            sphere{<+e/2,+b/2-h+d/2,-a/2> (s/2-a/2)/2}
            sphere{<-e/2,+b/2-h+d/2,+a/2> (s/2-a/2)/2}
